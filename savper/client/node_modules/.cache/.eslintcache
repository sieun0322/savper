Value={setPasswordCheck}\r\n        />\r\n        <button type=\"submit\">회원가입</button>\r\n      </form>\r\n    </div>\r\n  );\r\n};\r\nexport default RegisterPage;\r\n","C:\\Users\\lce95\\Desktop\\savper\\savper\\client\\src\\pages\\MainPage.js",["63"],"import React, { useState, useContext } from \"react\";\r\nimport UploadForm from \"../components/UploadForm\";\r\nimport DocList from \"../components/DocList\";\r\nimport { AuthContext } from \"../context/AuthContext\";\r\n\r\nconst MainPage = () => {\r\n  const [me] = useContext(AuthContext);\r\n  return <div><h2>Savper App</h2>\r\n      {me && <UploadForm />}\r\n      <DocList /></div>;\r\n};\r\nexport default MainPage;\r\n","C:\\Users\\lce95\\Desktop\\savper\\savper\\client\\src\\components\\ToolBar.js",[],"C:\\Users\\lce95\\Desktop\\savper\\savper\\client\\src\\components\\CustomInput.js",["64"],"import React, { useState } from \"react\";\r\n\r\nconst CustomInput = ({ label, value, setValue, type =\"text\"}) => {\r\n  return (\r\n    <div style={{ marginTop: 100, maxWidth: 350, margin: \"auto\" }}>\r\n      <label>{label}:</label>\r\n      <input\r\n        style={{ width: \"100%\" }}\r\n        value={value}\r\n        type={type}\r\n        onChange={(e) => setValue(e.target.value)}\r\n      />\r\n    </div>\r\n  );\r\n};\r\nexport default CustomInput;","C:\\Users\\lce95\\Desktop\\savper\\savper\\client\\src\\context\\AuthContext.js",[],"C:\\Users\\lce95\\Desktop\\savper\\savper\\client\\src\\pages\\DocPage.js",["65"],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":1,"column":29,"nodeType":"72","messageId":"73","endLine":1,"endColumn":37},{"ruleId":"74","severity":1,"message":"75","line":21,"column":20,"nodeType":"76","messageId":"77","endLine":21,"endColumn":22},{"ruleId":"70","severity":1,"message":"71","line":1,"column":17,"nodeType":"72","messageId":"73","endLine":1,"endColumn":25},{"ruleId":"70","severity":1,"message":"71","line":1,"col